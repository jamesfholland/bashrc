set splitright

set backspace=2
set nocompatible
set ruler
set showcmd
set showmode
set showmatch

set incsearch
set hlsearch
set ignorecase
set smartcase
set autoindent
set smartindent

set expandtab
set ts=4
set softtabstop=4
set shiftwidth=4

"set undofile
set cursorline
"set relativenumber
"set ttyfast
"set laststatus=2
"set wildmenu
"set wildmode=list:longest
colorscheme elflord

set runtimepath=~/.vim/,$VIMRUNTIME

filetype indent on

